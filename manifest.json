{
  "manifest_version": 2,

  "name": "MailIt",
  "description": "Mail on the go!",
  "version": "1.0",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },


  "permissions": [  
          "tabs",      
          "contextMenus",
          "notifications",
          "https://mail.google.com/*",
          "activeTab"

        ]
}
